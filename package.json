{
  "name": "schedule-backend",
  "version": "1.0.0",
  "description": "The official package of the schedule backend api",
  "main": "index.js",
  "scripts": {
    "start": "run-script-os",
    "start:win32": "set NODE_ENV=dev&&node app.js",
    "start:darwin:linux": "NODE_ENV=dev node app.js",
    "start:default": "NODE_ENV=dev node app.js",
    "dev": "run-script-os",
    "dev:win32": "set NODE_ENV=dev&&nodemon app.js",
    "dev:darwin:linux": "NODE_ENV=dev nodemon app.js",
    "dev:default": "NODE_ENV=dev nodemon app.js",
    "prod": "run-script-os",
    "prod:win32": "set NODE_ENV=prod&&node app.js",
    "prod:darwin:linux": "NODE_ENV=prod node app.js",
    "prod:default": "NODE_ENV=prod node app.js",
    "depcheck": "depcheck .",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DSCUAIC/schedule-backend.git"
  },
  "keywords": [
    "scheduler"
  ],
  "author": "Teodor Proca",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DSCUAIC/schedule-backend/issues"
  },
  "homepage": "https://github.com/DSCUAIC/schedule-backend#readme",
  "dependencies": {
    "@datadog/browser-logs": "^1.8.1",
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.0",
    "morgan": "^1.9.1",
    "rotating-file-stream": "^1.4.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0"
  },
  "devDependencies": {
    "depcheck": "^0.9.2",
    "nodemon": "^2.0.2",
    "run-script-os": "^1.1.1",
    "standard": "^14.3.1"
  }
}
